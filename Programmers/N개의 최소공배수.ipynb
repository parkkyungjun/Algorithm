   "id": "gentle-vector",
   "metadata": {},
   "outputs": [],
   "source": [
    "def gcd(x, y):\n",
    "    while y>0:\n",
    "        x, y = y, x%y\n",
    "    return x\n",
    "def lcm(x, y):\n",
    "    return x*y//gcd(x,y)\n",
    "\n",
    "def solution(arr):\n",
    "    s = arr[0]\n",
    "    for i in range(1, len(arr)):\n",
    "        s = lcm(s, arr[i])\n",
    "    return s"
   ]
  }
