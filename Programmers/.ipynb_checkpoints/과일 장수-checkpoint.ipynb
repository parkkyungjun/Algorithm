   "source": [
    "def solution(k, m, score):\n",
    "    k = len(score)%m\n",
    "    score = sorted(score)[k:]\n",
    "    answer = 0\n",
    "    for i in range(len(score)//m):\n",
    "        answer += min(score[i*m:(i+1)*m])*m\n",
    "    return answer"
   ]
  }
