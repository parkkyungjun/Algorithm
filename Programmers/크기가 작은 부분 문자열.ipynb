   "source": [
    "def solution(t, p):\n",
    "    a, b, c = len(t), len(p), int(p)\n",
    "    cnt = 0\n",
    "    \n",
    "    for i in range(a - b + 1):\n",
    "        if int(t[i:i+b]) <= c:\n",
    "            cnt += 1\n",
    "    return cnt"
   ]
  }
