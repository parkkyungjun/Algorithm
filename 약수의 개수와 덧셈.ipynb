   "source": [
    "def solution(left, right):\n",
    "    result = 0\n",
    "    for i in range(left,right+1):\n",
    "        if int(i**0.5) == i**0.5:\n",
    "            result -= i\n",
    "        else:\n",
    "            result += i\n",
    "    return result"
   ]
  }
