   "source": [
    "def solution(food):\n",
    "    food = [i//2 for i in  food[1:]]\n",
    "    left, right = '', ''\n",
    "    for index, i in enumerate(food):\n",
    "        left, right = left + str(index+1)*i, str(index+1)*i + right\n",
    "    return left+'0'+right"
   ]
  }
