   "source": [
    "from collections import Counter\n",
    "\n",
    "def solution(weights):\n",
    "    w = Counter(weights)\n",
    "    \n",
    "    return sum([(w[i]*(w[i]-1))//2+w[i]*w[i*2]+w[i]*w[i*2/3]+w[i]*w[i*3/4] for i in w.keys()])"
   ]
  }
