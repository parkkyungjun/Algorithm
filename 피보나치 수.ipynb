    "def solution(n):\n",
    "    a = [1 for i in range(n+1)]\n",
    "    a[0] = 0 \n",
    "    for i in range(n-2):\n",
    "        a[i+3] = a[i+1] + a[i+2]\n",
    "    return a[n]%1234567\n",
    "solution(5)"
   ]
  }
