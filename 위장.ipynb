   "source": [
    "from functools import reduce\n",
    "from collections import Counter\n",
    "def solution(clothes):\n",
    "    C = Counter([v for k, v in clothes])\n",
    "    return reduce(lambda x, y: x * (y+1), C.values(), 1) - 1"
   ]
