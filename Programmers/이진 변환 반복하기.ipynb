   "source": [
    "def solution(s):\n",
    "    cnt, xcnt = 0, 0\n",
    "    while s != '1':\n",
    "        xcnt += s.count('0')\n",
    "        s = s.replace('0', '')\n",
    "        s = bin(len(s))[2:]\n",
    "        cnt += 1\n",
    "    return [cnt, xcnt]"
   ]
  }
