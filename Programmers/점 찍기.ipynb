   "source": [
    "def solution(k, d):\n",
    "    cnt = 0\n",
    "    for i in range(1, d//k +1):\n",
    "        x = i*k\n",
    "        y = int((d**2 - x**2)**(1/2))\n",
    "        cnt += y//k\n",
    "    return cnt+(d//k)*2+1"
   ]
  }
